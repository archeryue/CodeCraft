      "id": "inspect-001",
      "description": "Get function symbol info",
      "tool": "InspectSymbol",
      "category": "happy_path",
      "tags": [
        "inspect",
        "function",
        "info"
      ],
      "difficulty": 1,
      "input": {
        "params": {
          "symbol": "hello",
          "file": "app.ts"
        }
      },
      "expected": {
        "success": true,
        "contains": "function"
      },
      "fixtures": {
        "type": "inline",
        "files": {
          "app.ts": "function hello() { return 'world'; }"
        }
      }
    },
