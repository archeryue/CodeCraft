{
  "tool": "llm_evaluation",
  "version": "1.0",
  "description": "LLM tool selection evaluation for file operations",
  "cases": [
    {
      "id": "llm-file-001",
      "description": "Read specific file",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "read"
      ],
      "difficulty": 1,
      "input": {
        "query": "Show me the contents of package.json"
      },
      "expected": {
        "expectedTool": "ReadFile",
        "forbiddenTools": [
          "WriteFile",
          "EditFile",
          "DeleteFile"
        ],
        "expectedParams": {
          "path": {
            "pattern": "package\\.json"
          }
        }
      }
    },
    {
      "id": "llm-file-002",
      "description": "Read file with extension",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "read"
      ],
      "difficulty": 1,
      "input": {
        "query": "Can you read the README.md file?"
      },
      "expected": {
        "expectedTool": "ReadFile",
        "forbiddenTools": [
          "WriteFile",
          "DeleteFile"
        ],
        "expectedParams": {
          "path": {
            "pattern": "README"
          }
        }
      }
    },
    {
      "id": "llm-file-003",
      "description": "Read file from subdirectory",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "read"
      ],
      "difficulty": 2,
      "input": {
        "query": "Show me src/agent.ts"
      },
      "expected": {
        "expectedTool": "ReadFile",
        "expectedParams": {
          "path": {
            "pattern": "src.*agent"
          }
        }
      }
    },
    {
      "id": "llm-file-004",
      "description": "Create new file",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "write"
      ],
      "difficulty": 1,
      "input": {
        "query": "Create a file called test.txt with the content 'Hello World'"
      },
      "expected": {
        "expectedTool": "WriteFile",
        "forbiddenTools": [
          "ReadFile",
          "DeleteFile"
        ],
        "expectedParams": {
          "path": {
            "contains": "test"
          },
          "content": {
            "contains": "Hello"
          }
        }
      }
    },
    {
      "id": "llm-file-005",
      "description": "Write configuration file",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "write"
      ],
      "difficulty": 2,
      "input": {
        "query": "Create a config.json file with empty object"
      },
      "expected": {
        "expectedTool": "WriteFile",
        "expectedParams": {
          "path": {
            "contains": "config"
          },
          "content": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "llm-file-006",
      "description": "Edit file - replace text",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "edit"
      ],
      "difficulty": 2,
      "input": {
        "query": "In test.txt, replace 'Hello' with 'Hi'"
      },
      "expected": {
        "expectedTool": "EditFile",
        "forbiddenTools": [
          "DeleteFile"
        ],
        "expectedParams": {
          "path": {
            "contains": "test"
          },
          "old_string": {
            "contains": "Hello"
          },
          "new_string": {
            "contains": "Hi"
          }
        }
      }
    },
    {
      "id": "llm-file-007",
      "description": "Edit file - update variable",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "file_ops",
        "edit"
      ],
      "difficulty": 2,
      "input": {
        "query": "Change the version in package.json from 1.0.0 to 1.1.0"
      },
      "expected": {
        "expectedTool": "EditFile",
        "acceptableTools": [
          "WriteFile"
        ],
        "expectedParams": {
          "path": {
            "contains": "package"
          }
        }
      }
    },
    {
      "id": "llm-file-008",
