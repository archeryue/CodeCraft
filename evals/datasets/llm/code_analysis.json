{
  "tool": "llm_evaluation",
  "version": "1.0",
  "description": "LLM tool selection evaluation for code analysis operations",
  "cases": [
    {
      "id": "llm-analysis-001",
      "description": "Inspect symbol information",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": [
        "analysis",
        "symbol"
      ],
      "difficulty": 2,
      "input": {
        "query": "Tell me about the executeTool symbol in src/tools.ts"
      },
      "expected": {
        "expectedTool": "InspectSymbol",
        "expectedParams": {
          "symbol": {
            "contains": "execute"
          },
          "file": {
            "contains": "tools"
          }
        }
      }
    },
    {
      "id": "llm-analysis-002",
