{
  "tool": "llm_evaluation",
  "version": "1.0",
  "description": "Bash command correctness evaluation - tests if the agent generates correct bash commands with proper syntax, flags, and parameters",
  "categories": {
    "git": "Git version control operations",
    "python": "Python script execution and pip",
    "node": "Node.js, npm, and npx operations",
    "file_ops": "File and directory operations (touch, mkdir, cp, mv, rm)",
    "network": "Network operations (curl, wget, ssh)",
    "process": "Process management (ps, kill, background)",
    "text": "Text processing (sed, awk, sort)",
    "docker": "Docker container operations",
    "build": "Build tools (make, cargo, go)",
    "system": "System info and utilities"
  },
  "cases": [
    {
      "id": "bash-git-001",
      "description": "Git commit with message",
      "tool": "llm_evaluation",
      "category": "llm_selection",
      "tags": ["git", "commit"],
      "difficulty": 1,
      "input": {
        "query": "Commit the staged changes with message 'fix: resolve login bug'"
      },
      "expected": {
        "expectedTool": "Bash",
        "expectedParams": {
          "command": {
            "pattern": "git\\s+commit\\s+(-m|--message)\\s+['\"]fix:\\s*resolve login bug['\"]"
          }
        }
      }
    },
    {
      "id": "bash-git-002",
